<!DOCTYPE html>
<html lang="en">

    <%- include('./partials/head.ejs') %> 
<body>


    <%- include('./partials/nav.ejs') %> 


    
    <div id="container">

        <h3>Register</h3>
        <form id="form-input" action="/signup" method ="post">
            <input type="text" name="fname" id="fname" placeholder="FirstName" required>
            <input type="text" name="lname" id="lname" placeholder="LastName" required>
            <input type="email" name="email" id="email" placeholder="Email" required>
            <input type="text" name="username" id="username"placeholder="Username" required>
            <input type="password" name="password" id="password" placeholder="Password" required>

            <button type="submit" value="Submit"><a href="/">SignUp</a> </button>
            <button type="submit"><a href="/login">LogIn</a></button>



        </form>
    </div>

   
    



    <script src="../js/index.js"></script>
</body>
</html>